{
	"name": "lilias-portfolio",
	"version": "0.0.5",
	"private": true,
	"type": "module",
	"scripts": {
		"commit": "cz",
		"release": "standard-version --no-verify",
		"dev": "npm-run-all --parallel typesafe-i18n dev:run",
		"dev:run": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.8.0",
		"@commitlint/config-conventional": "^17.8.0",
		"@commitlint/cz-commitlint": "^17.8.0",
		"@fontsource-variable/raleway": "^5.0.14",
		"@fontsource-variable/roboto-slab": "^5.0.15",
		"@fontsource/delius-unicase": "^5.0.15",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.7",
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.13.2",
		"autoprefixer": "^10.4.16",
		"axios": "^1.6.2",
		"commitizen": "^4.3.0",
		"daisyui": "^4.4.19",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"husky": "^8.0.3",
		"inquirer": "^9.2.12",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.32",
		"prettier": "^3.1.0",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"standard-version": "^9.5.0",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-copy": "^1.4.1",
		"svelte-feather-icons": "^4.0.1",
		"tailwind-merge": "^2.1.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "5.0.7",
		"vitest": "^1.0.2"
	},
	"dependencies": {
		"svelte-focus-trap": "^1.2.0",
		"svelte-french-toast": "^1.2.0",
		"typesafe-i18n": "^5.26.2"
	}
}
